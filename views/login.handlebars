<form action="" class="panel sign-form is-primary has-text-centered">
  <h1 class="panel-heading">Login</h1>
  <ul class="sign-form">
    <li class="not-radio">
      <label for="user">Username</label>
      <input
        class="input is-rounded is-primary"
        type="user"
        id="logUsernameInput"
        placeholder="Username"
      />
    </li>
    <li class="not-radio">
      <label for="password">Password</label>
      <input
        class="input is-rounded is-primary"
        type="password"
        id="logPasswordInput"
        placeholder="Password"
      />
    </li>
  </ul>
  <button class="button sign-form is-primary mb-5" type="submit">Login</button>
</form>

<script src="/public/javascript/login.js"></script>
